  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
		"moduleResolution": "Node",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "*.d.ts"],
  "exclude": ["node_modules", "build"]
}
